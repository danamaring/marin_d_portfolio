    <div>
      <div>
        <h1 id="contact-title">Say hi!</h1>
        <img id="contact-image" src="images/contact-image.jpg" alt="Phone hanging">
      </div>
      <div>
        <form action="post">
          <fieldset id="field">
            <input placeholder="Your Name" type="text">
          </fieldset>

          <fieldset id="field">
            <input placeholder="Your Email" type="email">
          </fieldset>

          <fieldset id="field-msg">
            <textarea placeholder="Your Message" type="text"></textarea>
          </fieldset>

          <fieldset>
            <button id="submit-button" name="submit" type="submit">Ready!</button>
          </fieldset>
        </form>
      </div>
    </div>
  </section>

  <footer id="footer">
    {{!-- Footer --}}
    <div>
      <img id="white-logo" src="images/logo-white.png" alt="Dana Logo">
